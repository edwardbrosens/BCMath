{
  "name": "edwardbrosens/bcmath",
  "description": "Convenience wrapper for the php bcmath extension.",
  "homepage": "https://github.com/edwardbrosens/BCMath",
  "license": "MIT",
  "version": "dev-master",
  "keywords": [
    "php", "bcmath", "arbitrary-precision", "class"
  ],
  "authors": [
    {
      "name": "Edward Brosens"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-bcmath": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*"
  },
  "autoload": {
    "psr-4": {
      "BCMath\\": "src/BCMath/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/edwardbrosens/BCMath"
    }
  ]
}